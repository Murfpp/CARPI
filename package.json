{
  "name": "CarpiTools",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "python-shell": "^5.0.0"
  },
  "build": {
    "appId": "com.example.carpitools",
    "files": [
      "dist/**/*",
      "_engine/**/*",
      "main.js",
      "index.html",
      "styles.css",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "_engine/",
        "to": "_engine/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis",
      "requestedExecutionLevel": "requireAdministrator"
    }
  }
}